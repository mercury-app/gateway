{
  "name": "merkoa",
  "version": "0.1.0",
  "description": "Middleware for mercuri to communicate with the backend.",
  "main": "src/server.js",
  "scripts": {
    "format": "prettier --write \"**/*.+(js|yml|yaml|json)\"",
    "lint": "eslint \"**/*.js\" --quiet --fix",
    "check": "npm run format && npm run lint",
    "start": "node src/server.js",
    "dev": "nodemon --watch 'src/**/*'",
    "test": "echo 'No tests added yet'"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-promise": "4.2.1",
    "husky": "2.3.0",
    "nodemon": "^1.19.0",
    "prettier": "1.17.1"
  },
  "dependencies": {
    "@koa/cors": "2.2.3",
    "axios": "^0.21.1",
    "dotenv": "8.0.0",
    "joi": "14.3.1",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-compress": "3.0.0",
    "koa-helmet": "4.1.0",
    "koa-router": "7.4.0",
    "uuid": "3.3.2"
  }
}
